script/source = "extends GridContainer
signal open(info)
onready var panel = $\"../../Panel\"
onready var left = $\"../../Left\"
onready var right = $\"../../Right\"
func _ready():
	pass # Replace with function body.


func setSlots(poke):
	var pokemon = poke
	for slot in get_children():
		if not pokemon.empty() and pokemon.front() != null:
			slot.pokeButton(pokemon.front())
			pokemon.remove(0)
#			print(\"full\")
		elif not pokemon.empty() and pokemon.front() == null:
			slot.pokeButton(null)
			pokemon.remove(0)
		else:
			break
	return pokemon
func setEmpty(id):
	for slot in get_children():
		if slot.id == null:
			slot.pokeButton(id)
			break
	

func isEmpty():
	var empty = true
	for slot in get_children():
		if slot.id != null:
			empty = false
	return empty

func isFull():
	var full = true
	for slot in get_children():
		if slot.id == null:
			full = false
	return full


func list_pokemon():
	var pokemon = []
	for slot in get_children():
		pokemon.push_back(slot.id)
	return pokemon
