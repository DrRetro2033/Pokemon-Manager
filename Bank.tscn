script/source = "extends Node
signal done_searching(data)
var pokemon

func _ready():
	pass # Replace with function body.


func set_data(data):
	pokemon = data.duplicate()
	print(pokemon)


func _on_Search_search(search_criteria):
	var array = []
	if search_criteria[\"nickname\"] != \"\":
		for x in pokemon:
			var pokemon_name = x.nickname
			if pokemon_name.findn(search_criteria[\"nickname\"]) >= 0:
				array.push_back(x)
	else:
		array = pokemon
	if search_criteria[\"type1\"] != 0:
		var temp_array = array.duplicate()
		for x in array:
			var type1 = x.type1
			if type1 != search_criteria[\"type1\"]:
				var pos = temp_array.find(x)
				print(pos)
				temp_array.remove(pos)
		array = temp_array.duplicate()
	if search_criteria[\"type2\"] != 0:
		var temp_array = array.duplicate()
		for x in array:
			var type2 = x.type2
			if type2 != search_criteria[\"type2\"]:
				var pos = temp_array.find(x)
				print(pos)
				temp_array.remove(pos)
		array = temp_array.duplicate()
	if search_criteria[\"gender\"] != 1:
		var temp_array = array.duplicate()
		for x in array:
			var gender = x.gender
			if gender != search_criteria[\"gender\"]:
				var pos = temp_array.find(x)
				print(pos)
				temp_array.remove(pos)
		array = temp_array.duplicate()
	print(array)
	$\"../Search\".showResults(array)
	
