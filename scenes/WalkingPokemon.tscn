script/source = "extends Node2D
const path = \"res://sprites/pokemon/icons/\"
signal next
signal pause(paused,ex)
# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"


# Called when the node enters the scene tree for the first time.
func _ready():
	$PokemonWalking.visible = true

func start(var info):
	var path_name = str(info[\"sprite\"])
	var temp_path = path+path_name
	print(temp_path)
	$PokemonWalking.set_texture(load(temp_path))
	if info[\"egg_groups\"].has(\"flying\"):
		$AnimationPlayer.play(\"Flying\")
	elif info[\"type1\"] == 14:
		$AnimationPlayer.play(\"Haunting\")
	elif info[\"egg_groups\"].has(\"water1\") or info[\"egg_groups\"].has(\"water2\") or info[\"egg_groups\"].has(\"water3\"):
		$AnimationPlayer.play(\"Swimming\")
	elif info[\"species\"] == 483:
		$AnimationPlayer.play(\"ZAWARUDO\")
	else:
		$AnimationPlayer.play(\"Walking\")
	$Timer.start()

func pause():
	$AnimationPlayer.stop(false)
	$Timer.stop()

func play():
	$AnimationPlayer.play()
	$Timer.start()

func _on_Timer_timeout():
	emit_signal(\"next\")

func pause_everyone_else(paused):
	emit_signal(\"pause\",paused,self)


func _on_AnimationPlayer_animation_finished(anim_name):
	queue_free()
"

